    name: program.py
    title: プログラム
    lang: python3
    default:
      import numpy as np
    answer: |-2
      import numpy as np
      A = np.array([[2,3],[3.,5]])
      y = np.array([1,-1])
      print(np.linalg.solve(A,y))

